@font-face {
    font-family: 'PacificoRegular';
    src: url('<%= asset_path('Pacifico-webfont.eot') %>');
    src: url('<%= asset_path('Pacifico-webfont.eot') + '?#iefix' %>') format('embedded-opentype'),
    url('<%= asset_path('Pacifico-webfont.woff') %>') format('woff'),
    url('<%= asset_path('Pacifico-webfont.ttf') %>') format('truetype'),
    url('<%= asset_path('Pacifico-webfont.svg#PacificoRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}

